execute pathogen#infect()
inoremap jk <ESC>
let mapleader = ","
filetype plugin indent on
syntax on
set encoding=utf-8
colorscheme Tomorrow-Night-Eighties
set guifont=Consolas:h15
nnoremap <C-J> <C-W><C-J>
nnoremap <C-K> <C-W><C-K>
nnoremap <C-L> <C-W><C-L>
nnoremap <C-H> <C-W><C-H>
set autoindent
set noexpandtab
set tabstop=4
set shiftwidth=4
